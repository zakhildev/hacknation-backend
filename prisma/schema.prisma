generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "sqlite"
}

model Cache {
  id Int @unique @default(autoincrement()) @id
  socketId String 
  cache String
}
